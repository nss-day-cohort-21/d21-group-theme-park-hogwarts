<div id="singleOutput">
    <p><a class="attractionName" href="#">{{name}}</a> <br><span class="attrType">({{attraction_type}})</span></p>
    <div class="hidden">
        {{!-- <h5>({{attraction_type}})</h5> --}}
        <p class="areaName">Where: {{area_name}}<br>
            <button type="button" class="btn btn-sm delBtn" id="removeBtn-{{id}}" href="#">Reducto</button><br><br>
